{
  "name": "sodium-typescript-example",
  "version": "0.0.1-alpha",
  "description": "Sodium Typescript Example",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --open --config webpack.dev.js",
    "dev:auto-reload": "cross-env NODE_ENV=dev-auto-reload webpack-dev-server --progress --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cp -R ./src/webpage/static ./dist/",
    "dist:server": "http-server ./dist -o"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "Stephen Blackheath",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/jest": "23.1.5",
    "@types/node": "10.5.2",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "awesome-typescript-loader": "5.2.0",
    "chokidar": "2.0.4",
    "clean-webpack-plugin": "0.1.19",
    "cors": "^2.8.4",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "express": "^4.21.2",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "install": "^0.12.1",
    "jest": "23.4.0",
    "npm": "^6.1.0",
    "npm-run-all": "4.1.3",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "serve-index": "^1.9.1",
    "shelljs": "0.8.5",
    "source-map-loader": "0.2.3",
    "style-loader": "0.21.0",
    "ts-jest": "23.0.0",
    "ts-node": "7.0.0",
    "tsconfig-paths": "3.4.2",
    "typescript": "2.9.2",
    "uglify-js": "3.4.4",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "3.1.11",
    "webpack-merge": "4.1.3"
  }
}
